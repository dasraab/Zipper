{
  "name": "dasraab/zipper",
  "type": "library",
  "description": "This is a little neat helper for the ZipArchive methods with handy functions",
  "keywords": [
    "laravel",
    "ZIP",
    "Archive"
  ],
  "homepage": "http://github.com/Chumper/zipper",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Benjamin Schneider ",
      "email": "bs@bitbird.de",
      "homepage": "http://www.bitbird.de",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "ext-zip": "*",
    "illuminate/support": "^5.8|^6.0|^7.0",
    "illuminate/filesystem": "^5.8|^6.0|^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0|^8.0",
    "mockery/mockery": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "Chumper\\Zipper\\": "src/Chumper/Zipper"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Chumper\\Zipper\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Chumper\\Zipper\\ZipperServiceProvider"
      ],
      "aliases": {
        "Zipper": "Chumper\\Zipper\\Zipper"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
